{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "updater:default",
    "core:window:allow-maximize",
    "core:window:allow-set-resizable",
    "core:window:allow-set-maximizable",
    "sql:default",
    "sql:allow-execute"
  ]
}
